    "ios": "8.0",
    "tvos": "10.0"
  },
  "requires_arc": [
    "POD_REQUIRES_ARC/*.m"
  ],
  "subspecs": [
    {
      "name": "Core",
      "xno_dependencies": {
        "FBSDKCoreKit/XNBasics": [
        ]
